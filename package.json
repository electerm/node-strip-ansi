{
  "name": "@electerm/strip-ansi",
  "version": "1.0.0",
  "description": "A port for strip-ansi to work with nodejs env",
  "main": "dist/strip-ansi",
  "module": "src/strip-ansi.ts",
  "types": "dist/strip-ansi",
  "scripts": {
    "test": "standard && jest --coverage",
    "release": "rm -rf dist && webpack --progress --config webpack.config.babel.js",
    "prepublishOnly": "npm run release",
    "coveralls": "npm run release && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electerm/node-strip-ansi.git"
  },
  "keywords": [
    "strip-ansi"
  ],
  "author": "Drake Zhao <drake.zhao@ringcentral.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electerm/node-strip-ansi/issues"
  },
  "homepage": "https://github.com/electerm/node-strip-ansi#readme",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@tsconfig/recommended": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "dotenv-override-true": "^6.2.2",
    "jest": "^26.6.3",
    "standard": "^16.0.3",
    "strip-ansi": "^7.0.1",
    "ts-loader": "^8.0.11",
    "ts-standard": "^9.0.0",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "./test"
    ],
    "setupFiles": [
      "dotenv-override-true/config"
    ],
    "silent": false
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*-compiled.js",
      "/dist/"
    ]
  },
  "directories": {
    "test": "test"
  }
}
